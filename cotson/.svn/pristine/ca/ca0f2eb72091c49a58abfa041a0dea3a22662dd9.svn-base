<h1>New experiment_machines</h1>

<% form_for(@experiment_machines) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :experiment_id %><br />
    <%= f.text_field :experiment_id %>
  </p>
  <p>
    <%= f.label :machine %><br />
    <%= f.text_field :machine %>
  </p>
  <p>
    <%= f.label :sandbox_id %><br />
    <%= f.text_field :sandbox_id %>
  </p>
  <p>
    <%= f.label :mediator %><br />
    <%= f.check_box :mediator %>
  </p>
  <p>
    <%= f.label :server %><br />
    <%= f.text_field :server %>
  </p>
  <p>
    <%= f.label :vnc_port %><br />
    <%= f.text_field :vnc_port %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', experiment_machines_path %>
