<h1>Listing experiment_events</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Experiment</th>
  </tr>

<% for experiment_event in @experiment_events %>
  <tr>
    <td><%=h experiment_event.description %></td>
    <td><%=h experiment_event.experiment_id %></td>
    <td><%= link_to 'Show', experiment_event %></td>
    <td><%= link_to 'Edit', edit_experiment_event_path(experiment_event) %></td>
    <td><%= link_to 'Destroy', experiment_event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New experiment_event', new_experiment_event_path %>
