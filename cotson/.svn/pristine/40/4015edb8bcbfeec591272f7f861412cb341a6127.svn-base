<h1>Editing experiment_event</h1>

<% form_for(@experiment_event) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :experiment_id %><br />
    <%= f.text_field :experiment_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @experiment_event %> |
<%= link_to 'Back', experiment_events_path %>
