$: << File.expand_path(File.dirname(__FILE__) + '/../share')

require 'cotson'

begin
	if !Execute.runs?(DB['screen_pid']) then
		puts "no existing execution running"
	else
		a=Send.command('ngo')
		puts a ? a : "could not connect to command socket"
	end
rescue Exception => e
	finish e
end
